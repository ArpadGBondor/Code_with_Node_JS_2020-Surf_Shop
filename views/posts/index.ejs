<h1>Posts Index</h1>

<% posts.forEach((post)=>{ %>
  <div class="">
    <h2><%= post.title %></h2>
    <div>Price: <%= post.price %></div>
    <div>Description: <%= post.description %></div>
    <div>Location: <%= post.location %></div>
    <div class=""><a href="/posts/<%= post.id %>">View more</a></div>
  </div>
<% }); %>
